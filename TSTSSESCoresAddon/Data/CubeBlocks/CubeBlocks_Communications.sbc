    <Definition xsi:type="MyObjectBuilder_RadioAntennaDefinition">
      <Id>
        <TypeId>RadioAntenna</TypeId>
        <SubtypeId>SupressionAntenna</SubtypeId>
      </Id>
      <DisplayName>Hailfire Supression Antenna (Super Heavy)</DisplayName>
      <Description>This block gives you the ability to block the use of hand drills/welders/grinders, as well as blocks.

â€¢ Max Range: 500m

Script provided by Blues Hailfire.</Description>
      <Icon>Textures\GUI\SupressionAntenna\SupressionAntenna.dds</Icon>
      <Icon>Textures\GUI\StaticGrid\StaticGrid.dds</Icon>
      <Icon>Textures\GUI\SupressionAntenna\Hailfire.dds</Icon>
      <CubeSize>Large</CubeSize>
      <Public>true</Public>
      <GuiVisible>true</GuiVisible>
      <BlockTopology>TriangleMesh</BlockTopology>
      <Size x="1" y="2" z="1"/>
      <ModelOffset x="0" y="0" z="0"/>
      <Model>Models\Cubes\Large\SupressionBeacon\SupressionBeacon.mwm</Model>
      <Components>
        <Component Subtype="Cerambet" Count="60" /> 
        <Component Subtype="TitaniumPlate" Count="260" /> 
        <Component Subtype="RebelComponent" Count="8"> <!-- RebelComponent -->
          <DeconstructId>   
            <TypeId>Ore</TypeId>   
            <SubtypeId>RebelComponentScrap</SubtypeId>   
          </DeconstructId>   
        </Component> 
        <Component Subtype="PalladiumComponent" Count="258" />
        <Component Subtype="Screw" Count="360" /> 
        <Component Subtype="LargeTube" Count="140" /> 
        <Component Subtype="SmallTube" Count="160" /> 
        <Component Subtype="Construction" Count="130" /> 
        <Component Subtype="Computer" Count="18" /> 
        <Component Subtype="PowerCell" Count="7">  
          <DeconstructId>  
            <TypeId>Ore</TypeId>  
            <SubtypeId>Scrap</SubtypeId>  
          </DeconstructId>  
        </Component>  
        <Component Subtype="RadioCommunication" Count="50" /> 
        <Component Subtype="Cerambet" Count="20" /> 
      </Components>
      <CriticalComponent Subtype="RadioCommunication" Index="0"/>
      <MountPoints>
        <MountPoint Side="Bottom" StartX="0.00" StartY="0.00" EndX="1.00" EndY="1.00"/>
      </MountPoints>
      <BlockPairName>SupressionAntenna</BlockPairName>
      <MirroringY>Z</MirroringY>
      <MirroringZ>Y</MirroringZ>
      <BuildTimeSeconds>40</BuildTimeSeconds>
      <BuildProgressModels>
        <Model BuildPercentUpperBound="0.33" File="Models\Cubes\Large\SupressionBeacon\SupressionBeacon_BS1.mwm"/>
        <Model BuildPercentUpperBound="0.67" File="Models\Cubes\Large\SupressionBeacon\SupressionBeacon_BS2.mwm"/>
        <Model BuildPercentUpperBound="1.00" File="Models\Cubes\Large\SupressionBeacon\SupressionBeacon_BS3.mwm"/>
      </BuildProgressModels>
      <DamageEffectName>Damage_Electrical_Damaged</DamageEffectName>
      <DamagedSound>ParticleElectrical</DamagedSound>

      <TieredUpdateTimes>
          <unsignedInt>120</unsignedInt>
          <unsignedInt>240</unsignedInt>
          <unsignedInt>480</unsignedInt>
      </TieredUpdateTimes>
      
      <!-- Antenna info-->
      <ResourceSinkGroup>Utility</ResourceSinkGroup>
      <MaxBroadcastRadius>500</MaxBroadcastRadius>
      <!-- <EnableLight>true</EnableLight> -->
      <Flare></Flare>
      <MaxBroadcastPowerDrainkW>20</MaxBroadcastPowerDrainkW>

      <EmissiveColorPreset>Default</EmissiveColorPreset>
      <DestroyEffect>BlockDestroyedExplosion_Large</DestroyEffect>
      <DestroySound>WepSmallWarheadExpl</DestroySound>
      <PCU>100</PCU>
      <IsAirTight>false</IsAirTight>          
    </Definition>
   


